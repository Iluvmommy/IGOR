<link
  href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css"
  rel="stylesheet"
  integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl"
  crossorigin="anonymous"
/>
<div class="m-3">
  <h2 class="text-primary">Sent To Firebase</h2>
  <div class="mx-3">
    <% if (url.includes("audio-ssl.itunes.apple.com")) { %>
      <video controls autoplay name="media">
        <source src="<%= url %>" type="audio/x-m4a">
      </video>
    <% } else { %>
      <iframe width="100%" height="500" src="<%= url %>"></iframe>
    <% } %>
    <a class="row" target="_blank" href="<%= url %>" url %>>Listen Link</a>
    <a class="row" href="/">Back To Search</a>
  </div>
</div>
